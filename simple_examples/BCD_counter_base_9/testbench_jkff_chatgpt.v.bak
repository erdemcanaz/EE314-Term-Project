module testbench_jkff_chatgpt;

  // Parameters
  parameter PERIOD = 10;  // Clock period in time units

  // Signals
  reg J;
  reg K;
  reg reset;
  reg clk;

  wire Q;
  wire Qbar;

  // Instantiate the JKFlipFlop module
  jkff_chatgpt jkff_chatgpt (
    .J(J),
    .K(K),
    .RST(RST),
    .CLK(CLK),
    .Q(Q),
    .NQ(NQ)
  );

  // Clock generation
  always begin
    clk = 0;
    #PERIOD / 2;
    clk = 1;
    #PERIOD / 2;
  end

  // Stimulus
  initial begin
    // Reset
    reset = 1;
    #PERIOD * 2;
    reset = 0;
    #PERIOD * 2;

    // Toggle Q
    J = 1;
    K = 1;
    #PERIOD;
    J = 0;
    K = 1;
    #PERIOD;
    J = 1;
    K = 0;
    #PERIOD;
    J = 1;
    K = 1;
    #PERIOD;

    // Finish simulation
    $finish;
  end

  // Display output
  always @(posedge clk) begin
    $display("Q = %b, Qbar = %b", Q, Qbar);
  end

endmodule
